    journal = {The Journal of Neuroscience},
    keywords = {brain},
    month = may,
    number = {22},
    pages = {9385--9393},
    pmid = {23719806},
    posted-at = {2013-11-28 08:00:02},
    priority = {2},
    publisher = {Society for Neuroscience},
    title = {{The Brain's Silent Messenger: Using Selective Attention to Decode Human Thought for Brain-Based Communication}},
