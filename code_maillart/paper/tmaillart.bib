    priority = {0},
    title = {{Heavy-tailed distribution of cyber-risks}},
    url = {http://epjb.edpsciences.org/index.php?option=com\_article\&\#38;access=standard\&\#38;Itemid=129\&\#38;url=/articles/epjb/abs/2010/11/b090956/b090956.html},
