    journal = {National Bureau of Economic Research Working Paper Series},
    keywords = {hawkes},
    month = mar,
    pages = {15850+},
    posted-at = {2012-07-13 14:53:29},
    priority = {2},
    title = {{Modeling Financial Contagion Using Mutually Exciting Jump Processes}},
