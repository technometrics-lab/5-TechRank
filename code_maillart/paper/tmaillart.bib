@article{slaughter1993,
    abstract = {{Implementation of a technology often requires solving complex problems. While the suppliers may expect these problems to occur, the users may be in a better position to both identify the exact nature of these problems and to solve them through their own innovations. In a detailed field-based study of the residential construction industry, I find that user-builders, rather than component manufacturers, are the developers of almost all of the innovations (n = 34) researched in depth. Using their expertise, the users quickly created low-cost, unique solutions to problems that appeared on-site, which were equivalent in cost and value to later manufacturer products. User-builders were also the exclusive source of innovations that concerned the integration of components. Manufacturers commercialized few user innovations, and excluded those that involved the connection of components. The prevalence of the user innovations and the lack of manufacturer commercialization indicates a de facto design partnership between users and manufacturers. The complementarity of learning-by-doing and user innovation during implementation has implications for the planning and management of the technology development process.}},
    author = {Slaughter, Sarah},
    citeulike-article-id = {11151636},
    citeulike-linkout-0 = {http://dx.doi.org/10.1016/0048-7333(93)90034-f},
    doi = {10.1016/0048-7333(93)90034-f},
