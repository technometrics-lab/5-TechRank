    posted-at = {2011-03-10 12:05:07},
    priority = {3},
    publisher = {ACM},
    series = {ACSAC '10},
    title = {{Familiarity breeds contempt: the honeymoon effect and the role of legacy code in zero-day vulnerabilities}},
