    posted-at = {2012-08-29 13:44:04},
    priority = {2},
    title = {{Profiting from voluntary information spillovers: how users benefit by freely revealing their innovations}},
    url = {http://dx.doi.org/10.1016/s0048-7333(03)00061-1},
    volume = {32},
    year = {2003}
