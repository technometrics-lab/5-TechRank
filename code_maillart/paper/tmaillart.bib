    citeulike-linkout-3 = {http://view.ncbi.nlm.nih.gov/pubmed/23719806},
    citeulike-linkout-4 = {http://www.hubmed.org/display.cgi?uids=23719806},
    day = {29},
    doi = {10.1523/jneurosci.5577-12.2013},
