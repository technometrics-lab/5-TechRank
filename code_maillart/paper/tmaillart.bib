    posted-at = {2012-07-24 07:17:45},
    priority = {2},
    publisher = {American Physical Society},
    title = {{Diffusion of epicenters of earthquake aftershocks, Omori's law, and generalized continuous-time random walk models}},
    url = {http://dx.doi.org/10.1103/physreve.66.061104},
    volume = {66},
