    issn = {03784371},
    journal = {Physica A: Statistical Mechanics and its Applications},
    keywords = {epidemics},
    month = feb,
    number = {3-4},
    pages = {577--591},
    posted-at = {2011-02-15 08:42:30},
    priority = {0},
    title = {{Endogenous versus exogenous shocks in systems with memory}},
    url = {http://dx.doi.org/10.1016/s0378-4371(02)01371-7},
    volume = {318},
    year = {2003}
}

